	<munit:test name="orderfeature-test-suite-Get:QuantityofSalesofPrevMonthbyStoreIdTest" doc:id="7827898d-a137-472b-8cd3-2ad0eae63d26" description="Test">
		<munit:behavior >
			<munit:set-event doc:name="Set GET | id=3" doc:id="539f43a0-9c29-4a70-845b-37182fd784c9" >
				<munit:attributes value="#[{ method : 'GET', queryParams : { id : '3'}}]" />
			</munit:set-event>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="Flow-ref to Get:QuantityofSalesofPrevMonthbyStoreId" doc:id="2b1dbb50-85ea-4244-ad5c-0dcfd8766826" name="Get:QuantityofSalesofPrevMonthbyStoreId"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Checks if Quantity of Sales is 49 when id = 3" doc:id="139b49f3-7c05-48d6-ac6a-16b2d6432b3d" actual='#[payload."Quantity of Sales"]' expected="49" message="The Quantity of Sales should be 49 for storeID '3'" />
		</munit:validation>
	</munit:test>
